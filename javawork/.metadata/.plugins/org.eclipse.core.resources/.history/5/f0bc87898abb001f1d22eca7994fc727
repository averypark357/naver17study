package day1216;

public class Ex10Gugudan {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		for(dan=2; dan<=9; dan++)
			System.out.print("[" + dan + "]"\t);
		System.out.println();
		System.out.println("=".repeat(50));
		for(int i=2; i<=9; i++)
		{
			for(int j=1; j<=9; j++)
			{
				System.out.println(i + " * " + j + " = " + i * j \t);
			}
			System.out.println();
		}
	}

}
