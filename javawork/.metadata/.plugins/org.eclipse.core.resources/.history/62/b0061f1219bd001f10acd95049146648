package day1218;

public class Car0 {
	//필드선언
	String model;
	Boolean start;
	int speed;
}

public class Book217 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//Car 객체 생성
		
		Car0 myCar = new Car0();
		
		//Car 객체의 필드값 읽기
		System.out.println("모델명: " + myCar.model);
		System.out.println("시동여부: " + myCar.start);
		System.out.println("현재속도: " + myCar.speed);
	}

}
