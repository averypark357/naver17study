package day1219;

public class Ex08StringBuffer {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		String str = "Happy";
		//String -> StringBuffer
		StringBuffer sb = new StringBuffer(str);
		System.out.println(sb); //인스턴스 변수인 경우 변수명만 출력시 자동으로 toString() 이 호출
		
	}

}
