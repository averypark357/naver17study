package day1220_2;

public class DbServerSystem {
	String dbServerName;
	int serverPort;
	
	public DbServerSystem() {
		// TODO Auto-generated constructor stub
		dbServerName = "Oracle";
		serverPort = 8080;
	}

	public DbServerSystem(String dbServerName, int serverPort)
	{
		this.dbServerName = dbServerName;
		this.serverPort = serverPort;
	}
}
