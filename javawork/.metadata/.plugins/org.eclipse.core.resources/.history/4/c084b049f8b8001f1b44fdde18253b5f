package day1213;

public class Ex06While {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		int a = 65;
		while(a <= 90) {
			//System.out.printf("%c", a++);
			System.out.print((char)a++);
		}
		
		System.out.println();
		char b = 'a';
		//do-while문을 이용해서 a부터 z까지 출력하시오
		do {
			System.out.print(b++);
		} while(b<='z');
		
		System.out.println();
		int n = 0;
		while(n<=10) {
			if(n%2==0)
				continue;
			System.out.printf("%3d", n);
		}
	}

}
