package day1212;

public class Ex05Operator {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		//조건(삼항)연산자 조건식 1? 참일때 값 : 거짓일때 값
		//조건이 여러개인 경우
		//조건식 1? 참일때 값 : 조건식 2? 참일때 값 : 조건식 3? 참일때 값 : 거짓일때 값
		int x,y,max;
		x=5;
		y=3;
		
		max=x>y?x:y;
		System.out.println("max=" + max);
	}

}
